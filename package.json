  "scripts": {
    "test": "echo 0"
  },
  "keywords": [
    "svg",
    "d3"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.3.5",
    "browser-sync": "^2.11.1",
    "del": "^2.2.0",
    "doiuse": "^2.3.0",
    "gulp": "^3.9.1",
    "gulp-description": "^0.1.2",
    "gulp-livereload": "^3.8.1",
    "gulp-load-plugins": "^1.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^6.1.0",
    "gulp-protractor": "^2.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-typescript": "^2.12.1",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "istanbul": "^0.4.2",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "jspm": "^0.17.0-beta.12",
    "karma": "^0.13.21",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.8",
    "karma-jspm": "^2.1.0",
    "lodash": "^4.6.1",
    "merge2": "^1.0.1",
    "npm": "^3.8.3",
    "postcss-reporter": "^1.3.3",
    "protractor": "^3.2.2",
    "remap-istanbul": "^0.5.1",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.5",
    "stylelint": "^5.2.1",
    "systemjs": "^0.19.24",
    "ts-node": "^0.7.1",
    "tsd": "^0.6.5",
    "typescript": "^1.8.7",
    "typescript-node": "^0.1.3",
    "typescript-register": "^1.1.0",
    "typings": "^0.7.9"
  },
  "dependencies": {
    "d3": "^3.5.16",
    "rxjs": "^5.0.0-beta.4"
  },
  "contributes": {
    "languages": [
      {
        "id": "css",
        "aliases": [
          "pcss",
          "postcss",
          "css"
        ],
        "extensions": [
          ".pcss"
        ]
      }
    ]
  },
  "jspm": {
    "name": "main",
    "directories": {
      "packages": "src/jspm_packages"
    },
    "configFiles": {
      "jspm:browser": "src/jspm.browser.js",
      "jspm": "src/jspm.config.js"
    },
    "dependencies": {
      "rxjs": "npm:@reactivex/rxjs@^5.0.0-beta.5",
      "css": "github:systemjs/plugin-css@^0.1.20",
      "d3": "npm:d3@^3.5.16"
    },
    "devDependencies": {
      "plugin-typescript": "github:frankwallis/plugin-typescript@^4.0.2"
    },
    "peerDependencies": {
      "assert": "github:jspm/nodelibs-assert@^0.2.0-alpha",
      "buffer": "github:jspm/nodelibs-buffer@^0.2.0-alpha",
      "child_process": "github:jspm/nodelibs-child_process@^0.2.0-alpha",
      "events": "github:jspm/nodelibs-events@^0.2.0-alpha",
      "fs": "github:jspm/nodelibs-fs@^0.2.0-alpha",
      "http": "github:jspm/nodelibs-http@^0.2.0-alpha",
      "https": "github:jspm/nodelibs-https@^0.2.0-alpha",
      "module": "github:jspm/nodelibs-module@^0.2.0-alpha",
      "os": "github:jspm/nodelibs-os@^0.2.0-alpha",
      "path": "github:jspm/nodelibs-path@^0.2.0-alpha",
      "process": "github:jspm/nodelibs-process@^0.2.0-alpha",
      "stream": "github:jspm/nodelibs-stream@^0.2.0-alpha",
      "url": "github:jspm/nodelibs-url@^0.2.0-alpha",
      "util": "github:jspm/nodelibs-util@^0.2.0-alpha"
    },
    "overrides": {
      "npm:d3@3.5.16": {
        "meta": {
          "d3.js": {
            "exports": "d3",
            "format": "global"
          }
        }
      },
      "npm:inherits@2.0.1": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:typescript@1.8.9": {
        "browser": {},
        "map": {
          "buffer": "@empty",
          "child_process": "@empty",
          "fs": "@empty",
          "os": "@empty",
          "path": "@empty",
          "process": "@empty",
          "readline": "@empty"
        }
      }
    }
  }
}
